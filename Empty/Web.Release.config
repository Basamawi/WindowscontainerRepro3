<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings xdt:Transform="Replace">
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="azure_application_settings" />
    <add name="DefaultConnectionAppEntities" providerName="System.Data.EntityClient" connectionString="azure_application_settings" />
  </connectionStrings>

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="On" xdt:Transform="Replace" />
  </system.web>
</configuration>